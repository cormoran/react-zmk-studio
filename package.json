  "keywords": [
    "react",
    "hooks",
    "zmk",
    "typescript",
    "mechanical-keyboards"
  ],
  "peerDependencies": {
    "react": ">=19.0.0"
  },
  "dependencies": {
    "@zmkfirmware/zmk-studio-ts-client": "github:cormoran/zmk-studio-ts-client#custom-studio-protocol"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
