    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "run-script-os": "^1.1.6",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0"
  }
}
