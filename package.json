{
  "name": "@cormoran/zmk-studio-react-hook",
  "version": "0.0.1",
  "description": "React hooks and components for ZMK Studio RPC",
  "license": "MIT",
  "author": {
    "name": "cormoran",
    "email": "cormoran@users.noreply.github.com",
    "url": "https://github.com/cormoran"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./testing": {
      "types": "./lib/testing/index.d.ts",
      "import": "./lib/testing/index.js"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "lint": "eslint . --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cormoran/zmk-studio-ts-client.git"
